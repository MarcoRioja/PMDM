<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="._1.MiPrimeraApp_9_1_24_.LoginEx_2">

    <Button
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:text="Aceptar"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/cen"
        android:id="@+id/izq"
        ></Button>
    <Button
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:text="Cancelar"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/izq"
        app:layout_constraintEnd_toEndOf="@+id/der"
        android:id="@+id/cen"
        ></Button>
    <Button
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:text="Salir"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/cen"
        android:id="@+id/der"
        ></Button>

</androidx.constraintlayout.widget.ConstraintLayout>